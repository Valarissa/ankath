<!DOCTYPE html>
<html>
<head>
  <title>Ankath</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="wrapper">
    <div class="ink_blot"></div>
    <div class="container">
      <div class="logo"></div>
      <%= render :partial => 'layouts/menu' %>
      <div class="flash_msgs">
        <p class="notice"><%= notice %></p>
        <p class="alert"><%= alert %></p>
      </div>
      <div class="content">
        <%= yield %>
      </div>
    </div>
  </div>
</body>
</html>
